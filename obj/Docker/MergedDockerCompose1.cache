networks:
  default:
    external:
      name: isolated
services:
  rabbitmq:
    image: rabbitmq:3-management
    ports:
    - 5672:5672/tcp
    - 15672:15672/tcp
  rabbitmq.publisher:
    build:
      context: D:\Docker test\RabbitMQ.Publisher
      dockerfile: RabbitMQ.Publisher/Dockerfile
    depends_on:
    - rabbitmq
    entrypoint: tail -f /dev/null
    environment:
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: rabbitmq.publisher:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  bin/Debug/netcoreapp2.0/RabbitMQ.Publisher.dll'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/bash -c "if PID=$$(pidof
        -x dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    volumes:
    - D:\Docker test\RabbitMQ.Publisher\RabbitMQ.Publisher:/app:rw
    - C:\Users\Shoieb\vsdbg\vs2017u5:/remote_debugger:ro
    - C:\Program Files\dotnet\sdk\NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - C:\Users\Shoieb\.nuget\packages:/root/.nuget/packages:ro
version: '3.0'